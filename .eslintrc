{
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },

  "env": {
    "browser": true,
    "node": true
  },

  "rules": {
    "no-unused-vars": "warn",
    "no-console": "warn",
    "no-undef": "warn",
    "no-unsafe-finally": "warn"
  }
}
